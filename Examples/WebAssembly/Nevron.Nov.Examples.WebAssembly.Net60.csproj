      <Name>Nevron.Nov.Grid</Name>
      <HintPath>..\..\Bin\Net80\Nevron.Nov.Grid.dll</HintPath>
      <Private>True</Private>
    </Reference>
    <Reference Include="Nevron.Nov.Host.WebAssembly">
      <Name>Nevron.Nov.Host.WebAssembly</Name>
      <HintPath>..\..\Bin\Net80\Nevron.Nov.Host.WebAssembly.dll</HintPath>
